{
  "name": "gltf2-viwer",
  "version": "1.0.0",
  "description": "a gltf2 viewer with webgl",
  "author": "https://github.com/kampfer",
  "license": "ISC",
  "scripts": {
    "dev": "webpack-dev-server --config scripts/webpack.dev.config.js",
    "dev:electron": "node scripts/electronDev.js",
    "dev:style": "webpack-dev-server --config scripts/webpack.style.config.js",
    "example": "webpack-dev-server --config scripts/webpack.example.config.js",
    "build:electron": "node ./scripts/electronBuild.js",
    "build:web": "webpack --config scripts/webpack.pro.config.js",
    "build": "npm run build:web && npm run build:electron",
    "analyse": "webpack --profile --json > stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "eslint": "^6.7.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
